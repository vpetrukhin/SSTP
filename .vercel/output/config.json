{
  "version": 3,
  "routes": [
    {
      "src": "^/(?:(.+)/)?index(?:\\.html)?/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "src": "^/(.*)\\.html/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/about$",
      "dest": "/about.html",
      "check": true
    }
  ],
  "overrides": {}
}